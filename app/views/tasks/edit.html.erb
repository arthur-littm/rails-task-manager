<div class="container">
<h1>Edit</h1>
<%= form_for(@task) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "control-form" %>

  <%= f.label :description %>
  <%= f.text_field :description, class: "control-form" %>

  <%= f.check_box :completed, class: "control-form"  %>

  <%= f.submit %>
<% end %>
</div>
